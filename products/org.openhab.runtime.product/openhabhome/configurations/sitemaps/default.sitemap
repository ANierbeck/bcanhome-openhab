			Image url="https://smartnest.dyndns.dk/cacti/graph_image.php?local_graph_id=7&rra_id=1"
		}
		Text label="Webcams" icon="contact-open" {
			Image url="https://smartnest.dyndns.dk/webcam/video.mjpg"
			Image url="https://smartnest.dyndns.dk/webcam2/jpg/image.jpg"
		}
	}
}
